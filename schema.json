{
  "allOf": [
    {
      "$ref": "https://geojson.org/schema/FeatureCollection.json"
    }
  ],
  "type": "object",
  "properties": {
    "features": {
      "type": "array",
      "items": {
        "title": "GeoJSON Feature",
        "type": "object",
        "required": [
          "properties"
        ],
        "properties": {
          "properties": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "pattern": "^([013-9]\\d|2[AB1-9])\\d{3}-T-\\d{3}$",
                "examples": [
                  "38185-T-001"
                ]
              },
              "nom": {
                "type": "string",
                "examples": [
                  "Gare - Frise"
                ]
              },
              "insee": {
                "type": "string",
                "pattern": "^([013-9]\\d|2[AB1-9])\\d{3}$",
                "examples": [
                  "38185"
                ]
              },
              "adresse": {
                "type": "string",
                "examples": [
                  "3 rue de la Gare, 92300, Levallois-Peret"
                ]
              },
              "emplacements": {
                "type": "integer",
                "examples": [
                  2
                ]
              },
              "no_appel": {
                "type": "string",
                "examples": [
                  "0476544254"
                ]
              },
              "info": {
                "type": "string",
                "examples": [
                  "Niveau -1 de la gare"
                ]
              }
            },
            "required": [
              "id",
              "nom",
              "insee"
            ]
          }
        }
      }
    }
  }
}
